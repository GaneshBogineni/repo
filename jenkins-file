pipeline {
  agent any{
  }
  stages {
    stage('print') {
        steps {

        echo 'Helloword'
              }
    }
    stage ('checkout') {
      steps {
        checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfig: [[url: 'https://github.com/GaneshBogineni/repo.git']]])
      }
   }
    stage ('build') {
      steps {
        sh "mvn clean verify"
        sh "mvn install"
      }
     }
    }
      post {
       always {
          echo " thanks for running the job "
         }
      }
    }
